{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "swagger": "tsoa spec-and-routes",
    "postinstall": "npm run gen:bereal-wrapper",
    "gen:bereal-wrapper": "swagger-typescript-api --axios -p ./src/openApiClients/beRealApi.json -o ./src/openApiClients/generated -n beRealApi.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "^4.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.4",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "swagger-typescript-api": "^11.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
